<script>
	document.addEventListener('DOMContentLoaded', function () {
		let user = localStorage.getItem('user');
		user = JSON.parse(user);
		// console.log(user.role);

		// Hide all sidebar items by default
		document.querySelectorAll('.menu-bar__one li').forEach(function (item) {
			item.style.display = 'none';
		});

		// Display specific sidebar items based on user role
		if (user.role === 'ADMIN') {
			document.querySelectorAll('.menu-bar__one li').forEach(function (item) {
				item.style.display = 'block';
			});
		} else if (user.role === 'USER') {
			// Show only specific items for USER role
			
			document.querySelector('a[href="/profile"]').closest('li').style.display = 'block';
			document.querySelector('a[href="/my-payments"]').closest('li').style.display = 'block';
			document.querySelector('a[href="/my-orders"]').closest('li').style.display = 'block';
		}
	});
</script>

  
